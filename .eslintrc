{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["react-app"],
  "plugins": ["prettier"],
  "rules": {
    "for-direction": "warn",
    "no-unused-vars": "warn",
    "no-var": "warn",
    "prefer-const": "warn",
    "camelcase": [
      "warn",
      {
        "ignoreDestructuring": true
      }
    ],
    "no-array-constructor": "warn",
    "require-await": "warn"
  }
}
